{
  "license": "MIT",
  "name": "eleventy",
  "private": true,
  "scripts": {
    "build": "run-s -l clean generate package",
    "clean": "rm -rf public",
    "generate": "eleventy",
    "package": "webpack --mode production",
    "build:serve": "run-s -l clean generate package serve:public",
    "serve:public": "npx http-server public",
    "dev": "run-p -l package:dev serve",
    "package:dev": "webpack --mode development --watch",
    "serve": "eleventy --serve",
    "debug-serve": "DEBUG=Eleventy* eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/stimulus-webpack-helpers": "^1.0.1",
    "@hotwired/turbo": "^7.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@tailwindcss/ui": "^0.7.2",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.5.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
